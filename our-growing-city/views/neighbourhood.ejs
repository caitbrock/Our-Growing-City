<%- include('./partials/header') %>

<section class="city - container">
    <h1> The Growing City - Toronto</h1>
    <h3> Stay up to date with development </br>
    happening in your neighbourhood </h3>
    
    <!-- map -->
    
    <table>
        <thead>
          <tr>
            <th>Street Number</th>
            <th>Street Name</th>
            <th>Explore</th>          
          </tr>
        </thead>
        <tbody>
          <% Data.forEach(function(n) { %>
          <tr>
            <td><%= n.STREET_NUM %> </td>
            <td><%= n.STREET_NAME %> </td>
            <td><a href="/neighbourhoods/<%= n._id %>">Details</a></td>
          </tr>
          <% }); %>
        </tbody>
      </table>

    <h3>Select your neighbourhood</h3>
</section>

<%- include('./partials/footer') %>